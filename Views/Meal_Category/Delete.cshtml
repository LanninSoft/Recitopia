@model Recitopia.Models.Meal_Category

@{
    ViewBag.Title = "Delete";
}
@if (ViewBag.ErrorMessage != null)
{
    <font style="color:red">@ViewBag.ErrorMessage</font>
}
<table style="width:100%" class="table table-borderless rounded-sm box effect8">
    <tr class="pageBannerAdmin">
        <th>
            Delete
        </th>
    </tr>
</table>
<br />
<div style="font-size: 25px; text-decoration:none">
    <font class="alert-danger">Are you sure you want to delete this Meal Category?</font>
</div>
<div>


    <dl class="dl-horizontal" style="font-size:13px">
        <dt>
            Category Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Category_Name)
        </dd>

        <dt>
            Notes
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Notes)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn alert-danger" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
